<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:dist="http://schemas.android.com/apk/distribution"
    package="com.Solver.Solver">

    <dist:module dist:instant="true" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_CONTACTS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/solverlogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/layoutbg"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:name=".ModelClass.Solver">
        <activity android:name=".Quotation_Show"
            android:parentActivityName=".Home"
            android:label="Quotation Display"
            ></activity>
        <activity android:name=".ForgotPasswordAtv" />
        <activity android:name=".SendQuotation" />
        <activity android:name=".Quotation_Request"
            android:parentActivityName=".Home"
            android:label="Quotation"
            />
        <activity android:name=".AddData" />
        <activity android:name=".ViewImage" />
        <activity android:name=".Daily_Report" />
        <activity android:name=".Schedule_Display"
            android:parentActivityName=".Home"
            android:label="Schedule"
            />
        <activity android:name=".Write_Schedule"
            android:parentActivityName=".Home"
            android:label="Write Schedule"
            />
        <activity android:name=".MessageActivity"
            android:parentActivityName=".Home"
            />
        <activity android:name=".GroupChatAtv"
         android:parentActivityName=".Home"
            />
        <activity android:name=".ClientShowDetails"
            android:parentActivityName=".Home"
            />
        <activity android:name=".ShowUserDetails"
            android:parentActivityName=".Home"
            />

        <activity
            android:name=".Home"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Client_reg"
            android:label="Client registration form" />
        <activity
            android:name=".Sign_Up"
            android:label="User registration form" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.theartofdev.edmodo.cropper.CropImageActivity" />

        <service
            android:name=".Service.FirebaseNotificationService"
            android:exported="false">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT" />
            </intent-filter>
        </service>

        <service android:name=".Notifications.MyFirebaseIdService"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.INSTANCE_ID_EVENT"/>
            </intent-filter>
        </service>

       <!-- <service android:name=".Notifications.MyFirebaseMessaging"
            android:enabled="true"
            android:exported="true">
            <intent-filter>
                <action android:name="com.google.firebase.MESSAGING_EVENT"/>
            </intent-filter>
        </service>
    -->
    <!--
           <service
               android:name=".Service.FirebaseNotificationService"
               android:permission="com.google.android.c2dm.permission.SEND">
               android:exported="false">
               <intent-filter>
                   <action android:name="com.google.firebase.MESSAGING_EVENT" />
                   <action android:name="com.google.android.c2dm.intent.RECEIVE" />
               </intent-filter>
           </service>   -->

    </application>

</manifest>